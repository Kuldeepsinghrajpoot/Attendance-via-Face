generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model student {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String
  fullName String
  password String
  uid      Int
}

model question {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  // uiid     Int    autoincrement()
  Question  String
  option1   String
  option2   String
  option3   String
  option4   String
  createdAt DateTime @default(now())
}
